                    </VBox>
                    <VBox>
                        <Label text="{i18n>MM_GTIN_MARM_STTPEC_GTIN}" tooltip="{i18n>MM_GTIN_MARM_STTPEC_GTIN}" class="MM_Label" required="false" wrapping="false"/>
                        <Input value="{MaterialDetails>data/MM_GTIN_MARM_STTPEC_GTIN}"  editable="false" class="MM_InputText" placeholder=""/>
