<core:FragmentDefinition xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core">
    <Dialog contentHeight="80%" contentWidth="80%" showHeader="true" id="id_AddNewUOM" draggable="true">
        <customHeader>
            <Bar>
                <contentLeft>
                    <Title level="H5" titleStyle="H5" text="{i18n>UNIT_OF_MEASURE}" class="MM_Title"/>
                </contentLeft>
                <contentRight>
                    <Button icon="sap-icon://decline" tooltip="close" press="fnCloseAddNewUOMPopUp"/>
                     <!-- visible="{parts:['MaterialDetails>/SystemDetails/AdditionalUOM/altUomListItem/addBtnClicked'],formatter: '.formatter.actionBtnAddNewUomVisibility'}" -->
