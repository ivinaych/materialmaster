                    <VBox>
                        <Label text="{i18n>MM_WEIGHT_UNIT_MARM_GEWEI}" tooltip="{i18n>MM_WEIGHT_UNIT_MARM_GEWEI}" class="MM_Label" required="{parts:['oAppModel>/sideNavigation/currentView', 'oAppModel>/taskDetails/wfTaskType'],formatter: '.formatter.fnWeightUnitRequired'}" wrapping="false"/>
                        <ComboBox selectedKey="{MaterialDetails>data/MM_WEIGHT_UNIT_MARM_GEWEI}" class="MM_ComboBox" width="100%" placeholder="" items="{LookupModel>/selectedSystemDataDropdown/MM_WEIGHT_UNIT_MARM_GEWEI}" showSecondaryValues="true" filterSecondaryValues= "true" valueState="{MaterialDetails>valueState/MM_WEIGHT_UNIT_MARM_GEWEI}" change="fnHandleComboboxValidation" editable="{parts:['MaterialDetails>data/MM_ALTERNATE_UNIT_MARM_MEINH','MaterialDetails>data/MM_BASE_UNIT_OF_MEASURE_MARA_MEINS'],formatter: '.formatter.fnAltUomDefaultRowEditability'}">
