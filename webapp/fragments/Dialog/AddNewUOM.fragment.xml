                    </VBox>
                    <VBox>
                        <Label text="{i18n>MM_BASE_UNIT_OF_MEASURE_MARA_MEINS}" tooltip="{i18n>MM_BASE_UNIT_OF_MEASURE_MARA_MEINS}" class="MM_Label" wrapping="false" required="true"/>
