                        <Input value="{MaterialDetails>data/MM_BP_TO_PROVIDE_SER_NUMB_MARM_STTPEC_SERNO_PROV_BUP}" class="MM_InputText" placeholder="" editable = "false"/>
                    </VBox>
                    <VBox>
                        <Label text="{i18n>MM_NATIONAL_CODE_MARM_STTPEC_NCODE}" tooltip="{i18n>MM_NATIONAL_CODE_MARM_STTPEC_NCODE}" class="MM_Label" required="false" wrapping="false"/>
                        <Input value="{MaterialDetails>data/MM_NATIONAL_CODE_MARM_STTPEC_NCODE}" class="MM_InputText" placeholder=""/>
                    </VBox>
