                         <ComboBox selectedKey="{MaterialDetails>data/MM_CN_MARM_NUMTP}" width="100%" items="{LookupModel>/selectedSystemDataDropdown/MM_EAN_CATEGORY_MARA_NUMTP}" showSecondaryValues="true" filterSecondaryValues= "true" class="MM_ComboBox" selectionChange ="onChangeeanCategory" editable="{parts:['MaterialDetails>data/MM_ALTERNATE_UNIT_MARM_MEINH','MaterialDetails>data/MM_BASE_UNIT_OF_MEASURE_MARA_MEINS'],formatter: '.formatter.fnAltUomDefaultRowEditability'}" change="fnHandleComboboxValidation">
                            <core:ListItem key="{LookupModel>EAN}" text="{LookupModel>EAN} - {LookupModel>Descrption}"/>
