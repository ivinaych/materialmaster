<core:FragmentDefinition xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core">
    <Dialog contentHeight="50%" contentWidth="50%" showHeader="true" id="id_ProductData_Tree" draggable="true">
        <customHeader>
            <Bar>
                <contentLeft>
                    <Title level="H5" titleStyle="H5" text="{parts:['MaterialDetails>/ProductDataStatic/TreeData/CurrentTree/headerName', 'LookupModel>/attributeListProdData'], formatter:'.formatter.getFieldNameFromi18OrAttributeService'}" class="MM_Title"/>
                </contentLeft>
            </Bar>
        </customHeader>
        <content>
            <VBox id="vboxTreeContainer">
                <!-- <Tree items="{MaterialDetails>/ProductDataStatic/TreeData/CurrentTree/lookupData}" mode="SingleSelectLeft" class="">
                    <CustomTreeItem selected="{parts:['MaterialDetails>nodeId', 'MaterialDetails>/ProductDataStatic/TreeData/CurrentTree/selectedNodeID'], formatter:'.formatter.onSetSelectedItemForTree'}">
                        <FlexBox alignItems="Start" width="100%">
                            <items>
                                <Text text="{MaterialDetails>text}"/>
                            </items>
                        </FlexBox>
                    </CustomTreeItem>
                </Tree> -->
            </VBox>
        </content>
        <footer>
        <OverflowToolbar>
            <ToolbarSpacer/>
            <Button text="{i18n>clear}" type="Default" class="MM_BtnDefault" press="onClearTreeFragment"></Button>
            <Button text="{i18n>okText}" type="Emphasized" class="MM_BtnEmphasized" press="onOkTreeFragment"></Button>
        </OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>
