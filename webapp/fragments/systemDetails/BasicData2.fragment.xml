                    <Title text="{i18n>clientSpecificConfig}" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <content>
                <l:Grid defaultSpan="XL3 L3 M6 S12" class="MM_Grid" binding="{MaterialDetails>/SystemDetails/basicData2/clientSpecificConfig}">
                    <VBox>
                        <Label text= "{i18n>MM_CROSS_PLANT_CM_MARA_SATNR}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_CROSS_PLANT_CM_MARA_SATNR', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <Input value="{MaterialDetails>data/MM_CROSS_PLANT_CM_MARA_SATNR}" width="100%" maxLength="18" class="MM_InputText" valueState="{MaterialDetails>valueState/MM_CROSS_PLANT_CM_MARA_SATNR}" editable= "{path: 'MaterialDetails>editable/MM_CROSS_PLANT_CM_MARA_SATNR', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}" liveChange="onAddingMandatoryValue"/>
