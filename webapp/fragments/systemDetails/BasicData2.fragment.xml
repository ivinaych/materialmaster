                    </VBox>
                    <VBox>
                        <Label text= "{i18n>MM_PAGE_NUMBER_MARA_BLATT}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_PAGE_NUMBER_MARA_BLATT', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <Input value="{MaterialDetails>data/MM_PAGE_NUMBER_MARA_BLATT}" width="100%" maxLength="3" class="MM_InputText" placeholder="{i18n>enter} {i18n>MM_PAGE_NUMBER_MARA_BLATT}" valueState="{MaterialDetails>valueState/MM_PAGE_NUMBER_MARA_BLATT}" editable= "{path: 'MaterialDetails>editable/MM_PAGE_NUMBER_MARA_BLATT', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}" liveChange="onAddingMandatoryValue"/>
