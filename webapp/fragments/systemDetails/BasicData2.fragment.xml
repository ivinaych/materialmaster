                    </VBox>
                    <VBox>
                        <Label text= "{i18n>MM_BASIC_MATERIAL_MARA_WRKST}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_BASIC_MATERIAL_MARA_WRKST', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <Input value="{MaterialDetails>data/MM_BASIC_MATERIAL_MARA_WRKST}" width="100%" maxLength="48" class="MM_InputText" placeholder="{i18n>enter} {i18n>MM_BASIC_MATERIAL_MARA_WRKST}" valueState="{MaterialDetails>valueState/MM_BASIC_MATERIAL_MARA_WRKST}" editable= "{path: 'MaterialDetails>editable/MM_BASIC_MATERIAL_MARA_WRKST', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}" liveChange="onAddingMandatoryValue"/>
