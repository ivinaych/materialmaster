                    <VBox>
                        <Label text= "{i18n>MM_DOCUMENT_MARA_ZEINR}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_DOCUMENT_MARA_ZEINR', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
