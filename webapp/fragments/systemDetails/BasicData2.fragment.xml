                        <CheckBox text="{i18n>MM_HIGHLY_VISCOS_MARA_IHIVI}" select="onSelectCheckbox" selected="{path: 'MaterialDetails>data/MM_HIGHLY_VISCOS_MARA_IHIVI', formatter:'.formatter.onSetBooleanValue'}" class="MM_CheckBox" editable= "{path: 'MaterialDetails>editable/MM_HIGHLY_VISCOS_MARA_IHIVI', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                    </VBox>
                </l:Grid>
            </content>
        </Panel>
