                    </VBox>
                    <VBox>
                        <Label text= "{i18n>MM_NO_SHEETS_MARA_BLANZ}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_NO_SHEETS_MARA_BLANZ', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
