                    <VBox>
                        <Label text= "{i18n>MM_IND_DESCRIPTION_MARA_NORMT}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_IND_DESCRIPTION_MARA_NORMT', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
