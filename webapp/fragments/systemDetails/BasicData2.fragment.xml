            <headerToolbar>
                <OverflowToolbar style="Clear">
                    <Title text="{i18n>environment}" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <content>
                <l:Grid defaultSpan="XL3 L3 M6 S12" class="MM_Grid" binding="{MaterialDetails>/SystemDetails/basicData2/environment}">
                    <VBox>
                        <Label text= "{i18n>MM_DG_INDICATOR_PROFILE_MARA_PROFL}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_DG_INDICATOR_PROFILE_MARA_PROFL', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <ComboBox selectedKey="{MaterialDetails>data/MM_DG_INDICATOR_PROFILE_MARA_PROFL}" width="100%" items="{LookupModel>/selectedSystemDataDropdown/MM_DG_INDICATOR_PROFILE_MARA_PROFL}"  class="MM_ComboBox" showSecondaryValues="true" filterSecondaryValues= "true" placeholder="{i18n>select} {i18n>MM_DG_INDICATOR_PROFILE_MARA_PROFL}" valueState="{MaterialDetails>valueState/MM_DG_INDICATOR_PROFILE_MARA_PROFL}" editable= "{path: 'MaterialDetails>editable/MM_DG_INDICATOR_PROFILE_MARA_PROFL', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"  selectionChange="onAddingMandatoryValue" change="fnHandleComboboxValidation">
                            <core:ListItem key="{LookupModel>DGIndProfile}" text="{LookupModel>DGIndProfile} - {LookupModel>Description}"/>
                        </ComboBox>
                    </VBox>
                    <VBox>
                        <Label text= "" class="MM_Label"/>
                        <CheckBox text="{i18n>MM_ENVIRONMENTALLY_RELEVANT_MARA_KZUMW}" select="onSelectCheckbox" selected="{path: 'MaterialDetails>data/MM_ENVIRONMENTALLY_RELEVANT_MARA_KZUMW', formatter:'.formatter.onSetBooleanValue'}" class="MM_CheckBox" editable= "{path: 'MaterialDetails>editable/MM_ENVIRONMENTALLY_RELEVANT_MARA_KZUMW', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                    </VBox>
                    <VBox>
                        <Label text= "" class="MM_Label"/>
                        <CheckBox text="{i18n>MM_IN_BULK_LIQUID_MARA_ILOOS}" select="onSelectCheckbox" selected="{path: 'MaterialDetails>data/MM_IN_BULK_LIQUID_MARA_ILOOS', formatter:'.formatter.onSetBooleanValue'}" class="MM_CheckBox" editable= "{path: 'MaterialDetails>editable/MM_IN_BULK_LIQUID_MARA_ILOOS', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
