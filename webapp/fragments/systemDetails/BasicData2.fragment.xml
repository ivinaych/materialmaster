                    </VBox>
                    <VBox>
                        <Label text= "{i18n>MM_DOCUMENT_TYPE_MARA_ZEIAR}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_DOCUMENT_TYPE_MARA_ZEIAR', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <Input value="{MaterialDetails>data/MM_DOCUMENT_TYPE_MARA_ZEIAR}" width="100%" maxLength="3" class="MM_InputText" placeholder="{i18n>enter} {i18n>MM_DOCUMENT_TYPE_MARA_ZEIAR}" valueState="{MaterialDetails>valueState/MM_DOCUMENT_TYPE_MARA_ZEIAR}" editable= "{path: 'MaterialDetails>editable/MM_DOCUMENT_TYPE_MARA_ZEIAR', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}" liveChange="onAddingMandatoryValue"/>
