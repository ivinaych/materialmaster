<core:FragmentDefinition xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:main="sap.ui.webc.main"
    xmlns:core="sap.ui.core">
    <VBox >
        <Panel expandable="true" width="auto" headerText="{i18n>otherData}" expanded="true" class="MM_panel sapUiSmallMarginEnd sapUiTinyMarginBottom">
            <headerToolbar>
                <OverflowToolbar style="Clear">
                    <Title text="{i18n>otherData}" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <content>
                <l:Grid defaultSpan="XL3 L3 M6 S12" class="MM_Grid" binding="{MaterialDetails>/SystemDetails/basicData2/otherData}">
                    <VBox>
                        <Label text= "{i18n>MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
                        <Input value="{MaterialDetails>data/MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH}" width="100%" maxLength="18" class="MM_InputText" placeholder="{i18n>enter} {i18n>MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH}" valueState="{MaterialDetails>valueState/MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH}" editable= "{path: 'MaterialDetails>editable/MM_PRODUCTION_INSPECTION_MEMO_MARA_FERTH', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"  liveChange="onAddingMandatoryValue"/>
                    </VBox>
                    <VBox>
                        <Label text= "{i18n>MM_PAGE_FORMAT_OF_PRODUCTION_MEMO_MARA_FORMT}" class="MM_Label" required="{path: 'MaterialDetails>required/MM_PAGE_FORMAT_OF_PRODUCTION_MEMO_MARA_FORMT', formatter:'.formatter.fnSetEditabilityforNonSAPFields'}"/>
